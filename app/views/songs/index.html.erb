<h1>Song List</h1>

<%= link_to "Create Song", new_song_path %>
<strong>Most Recent Song ID:</strong> <%= session[:most_recent_song_id] %>

<table>
  <tr>
    <th>Title</th>
    <th>Artist</th>
    <th>Actions</th>
  </tr>
  <% @songs.each do |song| %>
    <tr>
      <td><%= link_to song.title, song_path(song) %></td>
      <td><%= song.artist %></td>
      <td>
        <%= link_to "Edit", edit_song_path(song) %> |
        <%= link_to "Destroy", song_path(song), method: :delete %>
      </td>
    </tr>
  <% end %>
</table>
